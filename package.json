  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqhtiamo/imageComposer.git"
  },
  "keywords": [
    "image",
    "composer",
    "node",
    "canvas"
  ],
  "author": "cloughzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sqhtiamo/imageComposer/issues"
  },
  "homepage": "https://github.com/sqhtiamo/imageComposer#readme",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0"
  }
